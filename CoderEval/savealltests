    #         "rb")
    #     content = dill.load(f)
    #     f.close()
    #     print(type(content))
    #     print(type(content["input"]["args"][0]["bytes"]))
    #     object_class = dill.loads(content["input"]["args"][0]["bytes"])
    #     print(type(object_class))
    #     temp_class = Record()
    #     temp_class.__dict__.update(object_class)
