    if not text == "#foo/foo) #bar/bar] #hoo/hoo, #hee/hee.":
        isT = False
    source = "(#foo [#bar"
    tags, text = find_tags(source)
    if not tags == {"foo", "bar"}:
        isT = False
    if not text == source:
        isT = False
    tags, text = find_tags(source, replacer=_replacer)
    if not text == "(#foo/foo [#bar/bar":
        isT = False

    source = "#a!a #a#a #a$a #a%a #a^a #a&a #a*a #a+a #a.a #a,a #a@a #aa #a(a #a)a #a=a " \
             "#a?a #a`a #a'a #a\\a #a{a #a[a #a]a #a}a #a~a #a;a #a:a #a\"a #aa #aa #\xa0cd"
    tags, text = find_tags(source)
    if not tags == set():
        isT = False
    if not text == source:
        isT = False
    tags, text = find_tags(source, replacer=_replacer)
    if not text == source:
        isT = False
    source = '<p>First line</p><p>#foobar #barfoo</p>'
