    #     else:
    #         args0=content["input"]["args"][0]["bytes"]
    #     print(args0)
    #     # res0 = is_fill_request_seq(args0)
    #     # if not ( dill.dumps(res0)== dill.dumps(content["output"][0])):
    #     #     isT=False
    #     #     break
    if not isT:
        raise Exception("Result not True!!!")




