if __name__ == "__main__":
    isT = True
    invalid=[
        "minikube cluster 1",
        " minikube_cluster_1",
        "minikube_cluster_1 ",
        "minikube cluster_1",
        "minikube_cluster-",
        "-minikube_cluster_1",
        "minikube_cluster_1.",
        "-",
        ".",
        "",
        "a" * 500,
        "b" * 256,
    ]
    ist1=True
    ist2=True
    for l in invalid:
        try:
            _validate_resource_name(l)
            ist1=False
        except ValidationError as err:
            if err.messages!=['Invalid character in resource name.']:
                ist1=False

    valid=[
        "minikube_cluster_1",
        "minikube-cluster-1",
        "_minikube_cluster_",
        "minikube_cluster",
        "1_minikube_cluster_1",
        "534_minikube_cluster",
        "minikube_cluster_89982",
        "5",
        "a" * 255,
        "aa",
    ]
    for l in valid:
        try:
            if _validate_resource_name(l) is not None:
                ist2=False
        except:
            ist2=False
    if not ist1 or not ist2:
        isT=False
    if not isT:
        raise Exception("Result not True!!!")




