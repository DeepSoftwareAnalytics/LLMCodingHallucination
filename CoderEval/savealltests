    #     print(args0, args1, args2, args3)
    #     # Registered
    #     res0 = _verify(IDateClass, args1, args2, args3)
    #     # print(res0)
    #     # if not ( dill.dumps(res0)== dill.dumps(content["output"][0])):
    #     #     isT=False
    #     #     break
