        res2 = cache.setdefault(5, 2)
        res3 = cache.setdefault(7, 2)
        res4 = cache.setdefault(10, 7)
        res5 = cache.setdefault(6, 11)
        res6 = cache.setdefault(6, 20)
        if res1 != 10 or res2 != "five" or res3 != 2 or res4 != 7 or res5 != 600 or res6 != 600:
            isT = False
