    address_dict = add()
    max_connection_lifetime = 0
    temp_class = AsyncBolt3(address_dict, AsyncFakeSocket(address),
                            max_connection_lifetime)
    try:
        temp_class.begin()
        if temp_class.responses.popleft().message != "begin":
            isT=False
    except:
        isT=False
    if not isT:
        raise Exception("Result not True!!!")



