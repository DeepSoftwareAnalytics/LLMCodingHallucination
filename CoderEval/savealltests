    dictt={"key1":"value1","key2":"value2","key3":"value3"}

    temp_class = Record(dictt)
    if "key1" not in dictt.keys() or "key2" not in dictt.keys() or "key3" not in dictt.keys():
        isT=False

    if not isT:
        raise Exception("Result not True!!!")



